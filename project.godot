config_version=5

[application]

config/name="MvLO Mod Loader"
config/description="Customizable mod launcher and auto-updater for MvLO."
config/version="4"
run/main_scene="res://scenes/game_gallery.tscn"
config/features=PackedStringArray("4.2", "C#", "GL Compatibility")
run/low_processor_mode=true
run/low_processor_mode_sleep_usec=12000
boot_splash/bg_color=Color(0.180255, 0.180255, 0.180255, 1)
config/icon="res://audiovisual/nshbBig.png"

[autoload]

SelfUpdater="*res://global/self_updater.tscn"
Configurator="*res://global/configurator.gd"
ContentGetter="*res://global/content_getter.tscn"
InstallsIndex="*res://global/installs_index.tscn"
ArchiveHandler="*res://global/ArchiveHandler.cs"
DiscordHandler="*res://global/DiscordHandler.cs"

[display]

window/size/viewport_width=1024
window/size/viewport_height=768
window/energy_saving/keep_screen_on=false
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/handheld/orientation=6
window/vsync/vsync_mode=0
mouse_cursor/tooltip_position_offset=Vector2(20, 20)

[dotnet]

project/assembly_name="MvLO Mod Loader"

[filesystem]

import/blender/enabled=false

[gui]

theme/custom="res://ui_resources/theme_dark.tres"
theme/custom_font="res://ui_resources/font_roboto.ttf"
theme/default_font_hinting=2
theme/default_font_subpixel_positioning=0

