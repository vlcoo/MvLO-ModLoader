; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="MvLO Mod Loader"
config/description="Customizable mod launcher and auto-updater for MvLO."
run/main_scene="res://scenes/game_gallery.tscn"
config/features=PackedStringArray("4.0", "GL Compatibility")
config/icon="res://graphics/nshbBig.png"

[autoload]

Configurator="*res://global/configurator.gd"
ContentGetter="*res://global/content_getter.tscn"

[display]

window/size/viewport_width=1024
window/size/viewport_height=768
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray()

[gui]

theme/custom="res://ui_resources/theme_default.tres"
theme/custom_font="res://ui_resources/font_roboto.ttf"
theme/default_font_hinting=2
theme/default_font_subpixel_positioning=0

[internationalization]

locale/translation_remaps={}

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
